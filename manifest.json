{
  "manifest_version": 3,
  "name": "Youtube Ads Blocker",
  "description": "Youtube Ads is too good to be shown",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "contentScript.js"
      ]
    }
  ]
}